<script>
    const viewButtons = document.querySelectorAll('.data');
const overlay = document.getElementById('hs-viewUser');
const inputFields = overlay.querySelectorAll('input, textarea');

viewButtons.forEach(button => {
    button.addEventListener('click', (event) => {
        const idParts = button.id.split('-');
        const formulirId = idParts[idParts.length - 1]; // Assuming the alumni ID is the last part of the checkbox ID

            fetch(`/admin/form/${formulirId}`, {
                method: 'GET',
                
            })
        .then(response => response.json())
        .then(selectedRecord => {
            // Populate the overlay input fields with the fetched data (selectedRecord)
            document.getElementById('nama').value = selectedRecord.nama;
            document.getElementById('nim').value = selectedRecord.nim;
            document.getElementById('email').value = selectedRecord.email;
            document.getElementById('nomorkontak').value = selectedRecord.nomorkontak;
            document.getElementById('angkatanlulus').value = selectedRecord.angkatanlulus;
            document.getElementById('prodi_s1').value = selectedRecord.prodi_s1;
            document.getElementById('prodi_s2').value = selectedRecord.prodi_s2 || '';
            document.getElementById('prodi_s3').value = selectedRecord.prodi_s3 || '';
            document.getElementById('pend_profesi').value = selectedRecord.pend_profesi || '';
            document.getElementById('fakultas').value = selectedRecord.fakultas;
            document.getElementById('provinsi').value = selectedRecord.provinsi;
            document.getElementById('tempat_bekerja').value = selectedRecord.tempat_bekerja || '';
            document.getElementById('jabatan').value = selectedRecord.jabatan || '';
            document.getElementById('alamat_domis').value = selectedRecord.alamat_domis;
            document.getElementById('hobi').value = selectedRecord.hobi || '';
            document.getElementById('masukan_saran').value = selectedRecord.masukan_saran || '';
            const createdAtDate = new Date(selectedRecord.createdAt);
            const createdAtFormatted = `${createdAtDate.getDate()}/${createdAtDate.getMonth() + 1}/${createdAtDate.getFullYear()}`;
            document.getElementById('createdAt').value = createdAtFormatted;

            const updatedAtDate = new Date(selectedRecord.updatedAt);
            const updatedAtFormatted = `${updatedAtDate.getDate()}/${updatedAtDate.getMonth() + 1}/${updatedAtDate.getFullYear()}`;
            document.getElementById('updatedAt').value = updatedAtFormatted;

            // Show the overlay after populating the data
            HSOverlay.open(overlay);
        })
        .catch(error => {
            console.error('Error fetching data:', error);
            // Show an error message to the user (e.g., an alert)
        });
    });
});
</script>
<script>
    document.getElementById('generateExcelButton').addEventListener('click', function() {
        fetch('/admin/form/generateexcel', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            
            },
    
        })
        .then(response => {
            if (!response.ok) {
                return response.json().then(err => { throw err; });
            }
            return response.blob();
        })
        .then(blob => {
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.style.display = 'none';
            a.href = url;
            a.download = 'formulir_alumni_data.xlsx';
            document.body.appendChild(a);
            a.click();
            window.URL.revokeObjectURL(url);
        })
        .catch(error => {
            console.error('Error:', error);
            alert('Error generating Excel file: ' + (error.error || error.message || 'Unknown error'));
        });
    });
    </script>

